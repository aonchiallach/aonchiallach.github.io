start_date <- (as.POSIXct('1991/11/11 00:00:00 GMT') + ((as.POSIXct('1991/11/11 00:00:00 GMT') - Sys.time()) / 2) * -1)
#create target, which calculates the amount of words I should have written by now
target <- as.integer(((difftime(Sys.Date(), start_date) / 365) * 250) * 1000)
#I started school in September 1996 and I was in there until September 2015
education_days <- ((as.integer((difftime(as.POSIXct('2015/09/25 00:00:00 GMT'), start_date) + difftime(as.POSIXct('2021/09/21 00:00:00 GMT'), as.POSIXct('2016/08/01 00:00:00 GMT'))))  / 365) * 250)
